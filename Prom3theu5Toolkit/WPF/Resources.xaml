<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converters="clr-namespace:PTK.WPF.Converters">
    <converters:BooleanToVisibilityConverter x:Key="TrueToVisibleConverter" />
    <converters:BooleanToVisibilityConverter x:Key="FalseToVisibleConverter" IsFlipped="True" />
    <converters:MultiBooleanToVisibilityConverter x:Key="MultiTrueToVisibleConverter"/>
    <converters:StringToVisibilityConverter x:Key="NullStringToHiddenConverter" />
    <converters:ObjectToBoolConverter x:Key="NullObjectToFalseConverter" />
    <converters:BooleanToTextWrappingConverter x:Key="TrueToWrapConverter" />
    <converters:InverseBooleanConverter x:Key="InverseBooleanConverter"/>
</ResourceDictionary>